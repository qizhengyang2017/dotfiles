FQ2=~/work/ravi/re-sequencing/SRR8925023_2.fastq.gz
echo $FQ1
echo $FQ2
# fq2bam, bwa mem->sort->dep, 输出排序去重后的bam文件
singularity exec --nv $IMAGE/parabricks-4.4.0-1.sig pbrun fq2bam  --ref $REF --in-fq $FQ1 $FQ2  --out-bam $OUTBAM
cudaSetDevice()
export CUDA_VISIBLE_DEVICES=0
singularity exec --nv $IMAGE/parabricks-4.4.0-1.sig pbrun fq2bam  --ref $REF --in-fq $FQ1 $FQ2  --out-bam $OUTBAM
echo $CUDA_VISIBLE_DEVICES
singularity exec --nv $IMAGE/parabricks-4.4.0-1.sig pbrun fq2bam  --ref $REF --in-fq $FQ1 $FQ2  --out-bam $OUTBAM --gpu 0
singularity exec --nv $IMAGE/parabricks-4.4.0-1.sig --gpu 0 pbrun fq2bam  --ref $REF --in-fq $FQ1 $FQ2  --out-bam $OUTBAM
singularity exec --nv $IMAGE/parabricks-4.4.0-1.sig pbrun fq2bam -h 
singularity exec --nv $IMAGE/parabricks-4.4.0-1.sig --gpu 0 pbrun fq2bam  --ref $REF --in-fq $FQ1 $FQ2  --out-bam $OUTBAM --num-gpus 1
singularity exec --nv $IMAGE/parabricks-4.4.0-1.sig pbrun fq2bam  --ref $REF --in-fq $FQ1 $FQ2  --out-bam $OUTBAM --num-gpus 1
exit
